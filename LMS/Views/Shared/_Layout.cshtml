                                    <li>@Html.ActionLink("For Submission", "Contact", "Home")</li>
                                    <li>@Html.ActionLink("Submitted", "Contact", "Home")</li>
                                    <li>@Html.ActionLink("Cancelled", "Contact", "Home")</li>
